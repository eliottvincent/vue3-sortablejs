{
  "name": "vue3-sortablejs",
  "version": "1.0.7",
  "description": "A directive for Sortable on Vue 3",
  "author": "Eliott Vincent <dev@eliottvincent.com>",
  "homepage": "https://github.com/eliottvincent/vue3-sortablejs",
  "repository": {
    "type": "git",
    "url": "git://github.com/eliottvincent/vue3-sortablejs"
  },
  "bugs": {
    "url": "https://github.com/eliottvincent/vue3-sortablejs/issues"
  },
  "main": "dist/vue3-sortablejs.cjs.js",
  "module": "dist/vue3-sortablejs.esm-bundler.js",
  "browser": "dist/vue3-sortablejs.esm-browser.js",
  "exports": {
    ".": {
      "module": "./dist/vue3-sortablejs.esm-bundler.js",
      "require": "./dist/vue3-sortablejs.cjs.js",
      "import": "./dist/vue3-sortablejs.mjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node res/scripts/build.js",
    "lint": "eslint src --fix",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest ./test"
  },
  "dependencies": {
    "sortablejs": "1.15.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.20.2",
    "@rollup/plugin-commonjs": "13.0.2",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-replace": "5.0.02",
    "@rollup/plugin-terser": "0.2.1",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/test-utils": "2.3.0",
    "@vue/vue3-jest": "29.2.2",
    "babel-jest": "29.4.3",
    "eslint": "7.32.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "8.5.0",
    "jest": "27.5.1",
    "prettier": "1.19.1",
    "rollup": "2.79.1",
    "vue": "3.2.47"
  },
  "keywords": [
    "vue",
    "vue3",
    "vuejs",
    "sortable",
    "sortablejs",
    "draggable",
    "directive",
    "drag-and-drop",
    "drag",
    "drop"
  ],
  "license": "MIT"
}
